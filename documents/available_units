currently supported units:

temperature:
	- celcius
	- farenheit
	- kelvin

length:
	- meter
	- feet

mass:
	- kilograms
	- pounds
	- stones

speed:
	- miles per hour
	- kilometers per hour

area:
	- square meter
	- square feet

volume:
	- liter
	- gallon

time:
	- picoseconds
	- nanoseconds
	- microseconds
	- milliseconds
	- seconds
	- minutes
	- hours
	- days
	- weeks
	- months
	- years